{
  "name": "@architect/functions",
  "version": "3.2.1",
  "description": "Cloud function signatures for HTTP handlers, pub/sub + scheduled, queued functions, table triggers, and more",
  "homepage": "https://github.com/arc-repos/arc-functions",
  "repository": {
    "type": "git",
    "url": "https://github.com/arc-repos/arc-functions"
  },
  "bugs": "https://github.com/arc-repos/arc-functions/issues",
  "main": "src/index",
  "scripts": {
    "t": "tape test/proxy/*-test.js | tap-spec",
    "lint": "eslint ./src --fix",
    "test": "npm run lint && AWS_REGION=us-west-2 NODE_ENV=testing tape test/*-test.js | tap-spec && npm run clean",
    "clean": "rm -rf ./src/http/get-index"
  },
  "author": "Brian LeRoux <b@brian.io>",
  "license": "Apache-2.0",
  "keywords": [
    "aws",
    "lambda",
    "amazon",
    "serverless",
    "cloud",
    "cloudfunctions",
    "apigateway",
    "dynamodb",
    "s3",
    "sns",
    "api gateway",
    "framework"
  ],
  "dependencies": {
    "@architect/parser": "^1.1.7",
    "cookie": "^0.3.1",
    "cookie-signature": "^1.1.0",
    "csrf": "^3.1.0",
    "mime-types": "^2.1.24",
    "node-webtokens": "^1.0.0",
    "run-parallel": "^1.1.9",
    "run-waterfall": "^1.1.6",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "@architect/sandbox": "^1.0.10",
    "aws-sdk": "^2.480.0",
    "eslint": "^5.16.0",
    "mkdirp": "^0.5.1",
    "path-exists": "^4.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma:": "es5",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "printWidth": 1000
  }
}
